<Bindings>
	<Binding name="ALLTHETHINGS_OPENMAINLIST" header="ALLTHETHINGS" category="ADDONS">
		AllTheThings.OpenMainList();
	</Binding>
	<Binding name="ALLTHETHINGS_OPENMINILIST" category="ADDONS">
		AllTheThings.OpenMiniListForCurrentZone();
	</Binding>
	<Binding name="ALLTHETHINGS_OPENPROFESSIONMINILIST" category="ADDONS">
		AllTheThings.OpenMiniListForCurrentProfession();
	</Binding>
	<Binding name="ALLTHETHINGS_OPEN_RAID_ASSISTANT" category="ADDONS">
		AllTheThings:GetWindow("RaidAssistant"):Show();
	</Binding>
	<Binding name="ALLTHETHINGS_TOGGLEMAINLIST" category="ADDONS">
		AllTheThings.ToggleMainList();
	</Binding>
	<Binding name="ALLTHETHINGS_TOGGLEMINILIST" category="ADDONS">
		AllTheThings.ToggleMiniListForCurrentZone();
	</Binding>
	<Binding name="ALLTHETHINGS_TOGGLEBOEITEMS" category="ADDONS">
		AllTheThings.ToggleBOEItems();
	</Binding>
	<Binding name="ALLTHETHINGS_TOGGLECOMPLETIONISTMODE" category="ADDONS">
		AllTheThings.ToggleCompletionistMode();
	</Binding>
	<Binding name="ALLTHETHINGS_TOGGLEDEBUGMODE" category="ADDONS">
		AllTheThings.ToggleDebugMode();
	</Binding>
	<Binding name="ALLTHETHINGS_TOGGLE_RAID_ASSISTANT" category="ADDONS">
		AllTheThings:GetWindow("RaidAssistant"):Toggle();
	</Binding>
</Bindings>
